{
    "profile": "data-package",
    "name": "edm-i",
    "title": "Output EDM-I into premise",
    "description": "Implementation of EDM-I into premise",
    "version": "0.0.1",
    "contributors":[
    {
    "title": "Tom Terlouw",
    "email": "tom.terlouw@psi.ch"
    }],
    "dependencies": {
        "premise": ">=2.2.1"
    },
    "ecoinvent": {
        "version": "3.10",
        "system model": "cut-off"
    },
    "scenarios": [
        "NDC_LTT",
        "CBAM",
        "INDEPENDENCE"
    ],
    "licenses": [{
    "id": "CC0-1.0",
    "title": "CC0 1.0",
    "url": "https://creativecommons.org/publicdomain/zero/1.0/"
      }],
    "resources": [
        {
            "path": "scenario_data/scenario_data_edm_i.csv",
            "profile": "tabular-data-resource",
            "name": "scenario_data",
            "format": "csv",
            "mediatype": "text/csv",
            "encoding": "utf-8-sig",
            "schema": {
                "fields": [
                    {
                        "name": "model",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "scenario",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "region",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "variables",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "unit",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "2020",
                        "type": "number",
                        "format": "default"
                    },
                    {
                        "name": "2025",
                        "type": "number",
                        "format": "default"
                    },
                    {
                        "name": "2030",
                        "type": "number",
                        "format": "default"
                    },
                    {
                        "name": "2035",
                        "type": "number",
                        "format": "default"
                    },
                    {
                        "name": "2040",
                        "type": "number",
                        "format": "default"
                    },
                    {
                        "name": "2045",
                        "type": "number",
                        "format": "default"
                    },
                    {
                        "name": "2050",
                        "type": "number",
                        "format": "default"
                    }
                ],
                "missingValues": [
                    ""
                ]
            }
        },
        {
            "path": "configuration_file/config_edm_i.yaml",
            "profile": "data-resource",
            "name": "config",
            "format": "yaml",
            "mediatype": "text/yaml",
            "encoding": "utf-8"
        },
        {
            "path": "inventories/lci-edm-i.xlsx",
            "profile": "data-resource",
            "name": "inventories",
            "format": "csv",
            "mediatype": "text/csv",
            "encoding": "utf-8",
            "schema": {
                "fields": [
                    {
                        "name": "name",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "amount",
                        "type": "number",
                        "format": "default"
                    },
                    {
                        "name": "database",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "location",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "categories",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "unit",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "type",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "reference product",
                        "type": "string",
                        "format": "default"
                    }
                ],
                "missingValues": [
                    ""
                ]
            }
        }
    ]
}